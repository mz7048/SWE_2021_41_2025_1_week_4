        "s = set()\n",
        "\n",
        "def isHappy(n):\n",
        "  sum = 0\n",
        "  for i in range(0,len(str(n))):\n",
        "    sum += (n % 10) ** 2\n",
        "    n //= 10\n",
        "  if(sum in s):\n",
        "    return False\n",
        "  else:\n",
        "    s.add(sum)\n",
        "  if(sum == 1):\n",
        "    return True\n",
        "  else:\n",
        "    return isHappy(sum)\n",
        "\n",
        "num = int(input())\n",
        "print(isHappy(num))\n",
